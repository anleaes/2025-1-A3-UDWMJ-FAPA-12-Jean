{"ast":null,"code":"import { fetchServicos, createServico, updateServico, deleteServico } from '../services/api';\nexport default {\n  name: 'ServicosPage',\n  data() {\n    return {\n      nome: '',\n      servicos: [],\n      editando: false,\n      servicoEditandoId: null\n    };\n  },\n  methods: {\n    async listarServicos() {\n      const response = await fetchServicos();\n      this.servicos = response.data;\n    },\n    async salvarServico() {\n      if (this.nome.trim()) {\n        if (this.editando) {\n          await updateServico(this.servicoEditandoId, {\n            nome: this.nome\n          });\n        } else {\n          await createServico({\n            nome: this.nome\n          });\n        }\n        this.nome = '';\n        this.editando = false;\n        this.servicoEditandoId = null;\n        this.listarServicos();\n      }\n    },\n    prepararEdicao(servico) {\n      this.nome = servico.nome;\n      this.editando = true;\n      this.servicoEditandoId = servico.id;\n    },\n    async deletarServico(id) {\n      await deleteServico(id);\n      this.listarServicos();\n    }\n  },\n  mounted() {\n    this.listarServicos();\n  }\n};","map":{"version":3,"names":["fetchServicos","createServico","updateServico","deleteServico","name","data","nome","servicos","editando","servicoEditandoId","methods","listarServicos","response","salvarServico","trim","prepararEdicao","servico","id","deletarServico","mounted"],"sources":["C:\\projetos\\2025-1-A3-UDWMJ-FAPA-12-Jean\\frontend\\src\\views\\Servicos.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <h2>Cadastro de Serviços</h2>\r\n\r\n    <input v-model=\"nome\" placeholder=\"Nome do Serviço\" />\r\n    <button @click=\"salvarServico\">{{ editando ? 'Atualizar' : 'Adicionar' }}</button>\r\n\r\n    <ul>\r\n      <li v-for=\"servico in servicos\" :key=\"servico.id\">\r\n        {{ servico.nome }}\r\n        <button @click=\"prepararEdicao(servico)\">Editar</button>\r\n        <button @click=\"deletarServico(servico.id)\">Excluir</button>\r\n      </li>\r\n    </ul>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {\r\n  fetchServicos,\r\n  createServico,\r\n  updateServico,\r\n  deleteServico\r\n} from '../services/api'\r\n\r\nexport default {\r\n  name: 'ServicosPage',\r\n  data() {\r\n    return {\r\n      nome: '',\r\n      servicos: [],\r\n      editando: false,\r\n      servicoEditandoId: null\r\n    }\r\n  },\r\n  methods: {\r\n    async listarServicos() {\r\n      const response = await fetchServicos()\r\n      this.servicos = response.data\r\n    },\r\n    async salvarServico() {\r\n      if (this.nome.trim()) {\r\n        if (this.editando) {\r\n          await updateServico(this.servicoEditandoId, { nome: this.nome })\r\n        } else {\r\n          await createServico({ nome: this.nome })\r\n        }\r\n        this.nome = ''\r\n        this.editando = false\r\n        this.servicoEditandoId = null\r\n        this.listarServicos()\r\n      }\r\n    },\r\n    prepararEdicao(servico) {\r\n      this.nome = servico.nome\r\n      this.editando = true\r\n      this.servicoEditandoId = servico.id\r\n    },\r\n    async deletarServico(id) {\r\n      await deleteServico(id)\r\n      this.listarServicos()\r\n    }\r\n  },\r\n  mounted() {\r\n    this.listarServicos()\r\n  }\r\n}\r\n</script>\r\n\r\n"],"mappings":"AAkBA,SACEA,aAAa,EACbC,aAAa,EACbC,aAAa,EACbC,aAAY,QACP,iBAAgB;AAEvB,eAAe;EACbC,IAAI,EAAE,cAAc;EACpBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE,EAAE;MACRC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE,KAAK;MACfC,iBAAiB,EAAE;IACrB;EACF,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,cAAcA,CAAA,EAAG;MACrB,MAAMC,QAAO,GAAI,MAAMZ,aAAa,CAAC;MACrC,IAAI,CAACO,QAAO,GAAIK,QAAQ,CAACP,IAAG;IAC9B,CAAC;IACD,MAAMQ,aAAaA,CAAA,EAAG;MACpB,IAAI,IAAI,CAACP,IAAI,CAACQ,IAAI,CAAC,CAAC,EAAE;QACpB,IAAI,IAAI,CAACN,QAAQ,EAAE;UACjB,MAAMN,aAAa,CAAC,IAAI,CAACO,iBAAiB,EAAE;YAAEH,IAAI,EAAE,IAAI,CAACA;UAAK,CAAC;QACjE,OAAO;UACL,MAAML,aAAa,CAAC;YAAEK,IAAI,EAAE,IAAI,CAACA;UAAK,CAAC;QACzC;QACA,IAAI,CAACA,IAAG,GAA