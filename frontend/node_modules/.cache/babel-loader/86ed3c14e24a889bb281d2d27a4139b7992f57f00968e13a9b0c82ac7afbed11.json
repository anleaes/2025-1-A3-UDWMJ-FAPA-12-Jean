{"ast":null,"code":"import { fetchEquipamentos, createEquipamento, updateEquipamento, deleteEquipamento } from '../services/api';\nexport default {\n  name: 'EquipamentosPage',\n  data() {\n    return {\n      nome: '',\n      equipamentos: [],\n      editando: false,\n      equipamentoEditandoId: null\n    };\n  },\n  methods: {\n    async listarEquipamentos() {\n      const response = await fetchEquipamentos();\n      this.equipamentos = response.data;\n    },\n    async salvarEquipamento() {\n      if (this.nome.trim()) {\n        if (this.editando) {\n          await updateEquipamento(this.equipamentoEditandoId, {\n            nome: this.nome\n          });\n        } else {\n          await createEquipamento({\n            nome: this.nome\n          });\n        }\n        this.nome = '';\n        this.editando = false;\n        this.equipamentoEditandoId = null;\n        this.listarEquipamentos();\n      }\n    },\n    prepararEdicao(equipamento) {\n      this.nome = equipamento.nome;\n      this.editando = true;\n      this.equipamentoEditandoId = equipamento.id;\n    },\n    async deletarEquipamento(id) {\n      await deleteEquipamento(id);\n      this.listarEquipamentos();\n    }\n  },\n  mounted() {\n    this.listarEquipamentos();\n  }\n};","map":{"version":3,"names":["fetchEquipamentos","createEquipamento","updateEquipamento","deleteEquipamento","name","data","nome","equipamentos","editando","equipamentoEditandoId","methods","listarEquipamentos","response","salvarEquipamento","trim","prepararEdicao","equipamento","id","deletarEquipamento","mounted"],"sources":["C:\\projetos\\2025-1-A3-UDWMJ-FAPA-12-Jean\\frontend\\src\\views\\Equipamentos.vue"],"sourcesContent":["<template>\r\n  <div>\r\n    <h2>Cadastro de Equipamentos</h2>\r\n\r\n    <input v-model=\"nome\" placeholder=\"Nome do Equipamento\" />\r\n    <button @click=\"salvarEquipamento\">{{ editando ? 'Atualizar' : 'Adicionar' }}</button>\r\n\r\n    <ul>\r\n      <li v-for=\"equipamento in equipamentos\" :key=\"equipamento.id\">\r\n        {{ equipamento.nome }}\r\n        <button @click=\"prepararEdicao(equipamento)\">Editar</button>\r\n        <button @click=\"deletarEquipamento(equipamento.id)\">Excluir</button>\r\n      </li>\r\n    </ul>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {\r\n  fetchEquipamentos,\r\n  createEquipamento,\r\n  updateEquipamento,\r\n  deleteEquipamento\r\n} from '../services/api'\r\n\r\nexport default {\r\n  name: 'EquipamentosPage',\r\n  data() {\r\n    return {\r\n      nome: '',\r\n      equipamentos: [],\r\n      editando: false,\r\n      equipamentoEditandoId: null\r\n    }\r\n  },\r\n  methods: {\r\n    async listarEquipamentos() {\r\n      const response = await fetchEquipamentos()\r\n      this.equipamentos = response.data\r\n    },\r\n    async salvarEquipamento() {\r\n      if (this.nome.trim()) {\r\n        if (this.editando) {\r\n          await updateEquipamento(this.equipamentoEditandoId, { nome: this.nome })\r\n        } else {\r\n          await createEquipamento({ nome: this.nome })\r\n        }\r\n        this.nome = ''\r\n        this.editando = false\r\n        this.equipamentoEditandoId = null\r\n        this.listarEquipamentos()\r\n      }\r\n    },\r\n    prepararEdicao(equipamento) {\r\n      this.nome = equipamento.nome\r\n      this.editando = true\r\n      this.equipamentoEditandoId = equipamento.id\r\n    },\r\n    async deletarEquipamento(id) {\r\n      await deleteEquipamento(id)\r\n      this.listarEquipamentos()\r\n    }\r\n  },\r\n  mounted() {\r\n    this.listarEquipamentos()\r\n  }\r\n}\r\n</script>\r\n\r\n"],"mappings":"AAkBA,SACEA,iBAAiB,EACjBC,iBAAiB,EACjBC,iBAAiB,EACjBC,iBAAgB,QACX,iBAAgB;AAEvB,eAAe;EACbC,IAAI,EAAE,kBAAkB;EACxBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,IAAI,EAAE,EAAE;MACRC,YAAY,EAAE,EAAE;MAChBC,QAAQ,EAAE,KAAK;MACfC,qBAAqB,EAAE;IACzB;EACF,CAAC;EACDC,OAAO,EAAE;IACP,MAAMC,kBAAkBA,CAAA,EAAG;MACzB,MAAMC,QAAO,GAAI,MAAMZ,iBAAiB,CAAC;MACzC,IAAI,CAACO,YAAW,GAAIK,QAAQ,CAACP,IAAG;IAClC,CAAC